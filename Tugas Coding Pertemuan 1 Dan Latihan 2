def getAverage(mylist):
    sum = 0
    for item in mylist:
        sum = sum+item
    return sum

getAverage([1,2,3,4]) / 4
2.5

21.
def getkurang(mylist):
 kurang = 0
 for row in mylist:
 for item in row:
 kurang -= item
 return kurang
getkurang([[1,2,5],[3,4,7]])
from numpy.lib.function_base import average
A =[1,2,3,4]
B =[3,1,4,1]
def getAverage(mylist):
    total=getSum(mylist)
    average=total
    return average
